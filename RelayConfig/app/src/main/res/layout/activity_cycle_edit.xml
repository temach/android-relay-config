<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="ru.rele.relayconfig.CycleEditActivity">

    <ru.rele.relayconfig.CycleControl
        android:id="@+id/currentEditCycle"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </ru.rele.relayconfig.CycleControl>

    <LinearLayout
        android:id="@+id/timeStripsList"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </LinearLayout>

    <Button
        android:text="Add time strip"
        android:id="@+id/addTimeStrip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
